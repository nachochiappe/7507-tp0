Object subclass: #ComisionCurricularPermanente	instanceVariableNames: 'coleccionExpedientes indice'	classVariableNames: ''	poolDictionaries: ''	category: '90340'!!ComisionCurricularPermanente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 15:09'!votarAFavor	| expediente |	expediente := coleccionExpedientes at: indice.	expediente votarAFavor.! !!ComisionCurricularPermanente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 13:15'!recibirExpediente: unExpediente 	coleccionExpedientes add: unExpediente.! !!ComisionCurricularPermanente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 15:13'!votarEnContra	| expediente |	expediente := coleccionExpedientes at: indice.	expediente votarEnContra.! !!ComisionCurricularPermanente methodsFor: 'initialization' stamp: 'IgnacioChiappe 8/29/2018 15:22'!initialize	coleccionExpedientes := OrderedCollection new.	indice := 1.! !!ComisionCurricularPermanente methodsFor: 'processing' stamp: 'IgnacioChiappe 8/29/2018 15:18'!procesarExpediente	| expediente |	expediente := coleccionExpedientes at: indice.	expediente chequarAprobacion.	indice := indice + 1.	^expediente! !Object subclass: #Expediente	instanceVariableNames: 'nombre estado votosAFavor votosEnContra'	classVariableNames: ''	poolDictionaries: ''	category: '90340'!!Expediente methodsFor: 'initialization' stamp: 'IgnacioChiappe 8/29/2018 13:05'!initialize	estado := 'Iniciado'.	votosAFavor := 0.	votosEnContra := 0.! !!Expediente methodsFor: 'setting' stamp: 'IgnacioChiappe 8/29/2018 12:54'!establecerNombre: unNombre 	nombre := unNombre.! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 15:14'!votarAFavor	votosAFavor := votosAFavor + 1.! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 12:59'!estado	^estado.! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 13:50'!votosAFavor	^votosAFavor! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 15:21'!chequarAprobacion	(votosAFavor > votosEnContra) ifTrue: [ estado := 'Aprobado' ].	(votosAFavor > votosEnContra) ifFalse: [ estado := 'Rechazado' ].! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 13:50'!votosEnContra	^votosEnContra! !!Expediente methodsFor: 'accessing' stamp: 'IgnacioChiappe 8/29/2018 15:16'!votarEnContra	votosEnContra := votosEnContra + 1.! !